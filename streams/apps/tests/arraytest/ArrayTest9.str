/**
 * This tests resolving constant expressions within dimensions of
 * fields.
**/

void->void pipeline ArrayTest9 {
    add Source();
    add Test(10, 10);
    add Printer();
}

int->int filter Test(int N, int M) {
    // test field
    int[N*M*3] arr;

    init {
	arr[N*M*3-1] = 1;
    }

    work pop 1 push 1 {
	int x1 = pop();
	push(arr[N*M*3-1]*x1);
    }
}

void->int stateful filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}
